{
    "manifest_version": 3,
    "name": "MYTH",
    "short_name": "MYTH",
    "version": "1.1.2",
    "description": "High-Performance Offensive Intelligence Engine",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icons/128x128.png"
    },
    "permissions": [
        "nativeMessaging",
        "storage",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    }
}