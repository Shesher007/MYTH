{
    "manifest_version": 3,
    "name": "MYTH",
    "short_name": "MYTH",
    "version": "1.1.3",
    "description": "High-Performance Offensive Intelligence Engine",
    "start_url": ".",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "icons": {
        "128": "icon.png"
    },
    "permissions": [
        "storage",
        "notifications"
    ],
    "content_security_policy": {
        "extension_pages": "script-src 'self'; object-src 'self'"
    }
}